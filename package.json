{
    "name": "emmylua",
    "displayName": "EmmyLua",
    "description": "EmmyLua for vscode",
    "version": "0.9.28",
    "icon": "res/icon.png",
    "publisher": "tangzx",
    "engines": {
        "vscode": "^1.89.0"
    },
    "categories": [
        "Programming Languages",
        "Debuggers"
    ],
    "activationEvents": [
        "onDebug"
    ],
    "keywords": [
        "lua",
        "emmylua",
        "debugger"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/EmmyLua/VSCode-EmmyLua"
    },
    "bugs": {
        "url": "https://github.com/EmmyLua/VSCode-EmmyLua/issues"
    },
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "emmy.restartServer",
                "title": "EmmyLua: Restart Lua Server"
            },
            {
                "command": "emmy.insertEmmyDebugCode",
                "title": "EmmyLua: Insert Emmy Debugger Code"
            },
            {
                "command": "emmy.stopServer",
                "title": "EmmyLua: Stop EmmyLua Language Server"
            },
            {
                "command": "emmylua.luarocks.searchPackages",
                "title": "Search Packages",
                "category": "LuaRocks",
                "icon": "$(search)"
            },
            {
                "command": "emmylua.luarocks.installPackage",
                "title": "Install Package",
                "category": "LuaRocks",
                "icon": "$(cloud-download)"
            },
            {
                "command": "emmylua.luarocks.uninstallPackage",
                "title": "Uninstall Package",
                "category": "LuaRocks",
                "icon": "$(trash)"
            },
            {
                "command": "emmylua.luarocks.showPackageInfo",
                "title": "Show Package Info",
                "category": "LuaRocks",
                "icon": "$(info)"
            },
            {
                "command": "emmylua.luarocks.refreshPackages",
                "title": "Refresh",
                "category": "LuaRocks",
                "icon": "$(refresh)"
            },
            {
                "command": "emmylua.luarocks.showPackages",
                "title": "Show LuaRocks Packages",
                "category": "LuaRocks"
            },
            {
                "command": "emmylua.luarocks.clearSearch",
                "title": "Clear Search",
                "category": "LuaRocks",
                "icon": "$(clear-all)"
            },
            {
                "command": "emmylua.luarocks.checkInstallation",
                "title": "Check LuaRocks Installation",
                "category": "LuaRocks"
            }
        ],
        "languages": [
            {
                "id": "lua",
                "extensions": [
                    ".lua"
                ],
                "aliases": [
                    "EmmyLua"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "breakpoints": [
            {
                "language": "lua"
            }
        ],
        "jsonValidation": [
            {
                "fileMatch": ".emmyrc.json",
                "url": "emmyrc-schema://schemas/emmyrc"
            },
            {
                "fileMatch": ".emmyrc.json",
                "url": "./syntaxes/schema.json"
            }
        ],
        "debuggers": [
            {
                "type": "emmylua_attach",
                "label": "EmmyLua Attach Debug",
                "program": "./out/debugger/attach/EmmyAttachDebugAdapter.js",
                "runtime": "node",
                "configurationAttributes": {
                    "attach": {
                        "required": [
                            "pid"
                        ],
                        "properties": {
                            "pid": {
                                "type": "number",
                                "description": "%debug.attach.target_pid%",
                                "default": 0
                            },
                            "processName": {
                                "type": "string",
                                "description": "%debug.attach.target_process%",
                                "default": ""
                            },
                            "captureLog": {
                                "type": "boolean",
                                "description": "%debug.attach.captureLog%",
                                "default": false
                            }
                        }
                    }
                },
                "initialConfigurations": [
                    {
                        "type": "emmylua_attach",
                        "request": "attach",
                        "name": "%debug.attach.name%",
                        "pid": 0,
                        "processName": "",
                        "captureLog": false
                    }
                ],
                "configurationSnippets": [
                    {
                        "label": "%debug.attach.label%",
                        "description": "%debug.attach.desc%",
                        "body": {
                            "type": "emmylua_attach",
                            "request": "attach",
                            "name": "%debug.attach.name%",
                            "pid": 0,
                            "processName": ""
                        }
                    }
                ],
                "languages": [
                    "lua"
                ]
            },
            {
                "type": "emmylua_launch",
                "label": "EmmyLua Launch Debug",
                "program": "./out/debugger/launch/EmmyLaunchDebugAdapter.js",
                "runtime": "node",
                "configurationAttributes": {
                    "launch": {
                        "required": [
                            "program",
                            "workingDir",
                            "arguments",
                            "newWindow"
                        ],
                        "properties": {
                            "program": {
                                "type": "string",
                                "description": "%debug.launch.program%",
                                "default": ""
                            },
                            "workingDir": {
                                "type": "string",
                                "description": "%debug.launch.workingDir%",
                                "default": ""
                            },
                            "arguments": {
                                "type": "array",
                                "description": "%debug.launch.arguments%",
                                "default": []
                            },
                            "newWindow": {
                                "type": "boolean",
                                "description": "%debug.launch.newWindow%",
                                "default": false
                            }
                        }
                    }
                },
                "initialConfigurations": [
                    {
                        "type": "emmylua_launch",
                        "request": "launch",
                        "name": "%debug.launch.name%",
                        "program": "",
                        "workingDir": "",
                        "arguments": [],
                        "newWindow": false
                    }
                ],
                "configurationSnippets": [
                    {
                        "label": "%debug.launch.label%",
                        "description": "%debug.launch.desc%",
                        "body": {
                            "type": "emmylua_launch",
                            "request": "launch",
                            "name": "%debug.launch.name%",
                            "program": "",
                            "workingDir": "",
                            "arguments": [],
                            "newWindow": false
                        }
                    }
                ],
                "languages": [
                    "lua"
                ]
            },
            {
                "type": "emmylua_new",
                "label": "EmmyLua New Debugger",
                "program": "./out/debugger/new_debugger/EmmyNewDebugAdapter.js",
                "runtime": "node",
                "configurationAttributes": {
                    "launch": {
                        "required": [
                            "host",
                            "port",
                            "ideConnectDebugger"
                        ],
                        "properties": {
                            "host": {
                                "type": "string",
                                "description": "host",
                                "default": "localhost"
                            },
                            "port": {
                                "type": "number",
                                "description": "port",
                                "default": 9966
                            },
                            "ideConnectDebugger": {
                                "type": "boolean",
                                "description": "连接方式",
                                "default": true
                            },
                            "ext": {
                                "type": "array",
                                "description": "Lua文件后缀",
                                "default": [
                                    ".lua"
                                ]
                            }
                        }
                    }
                },
                "initialConfigurations": [
                    {
                        "type": "emmylua_new",
                        "request": "launch",
                        "name": "EmmyLua New Debug",
                        "host": "localhost",
                        "port": 9966,
                        "ext": [
                            ".lua",
                            ".lua.txt",
                            ".lua.bytes"
                        ],
                        "ideConnectDebugger": true
                    }
                ],
                "configurationSnippets": [
                    {
                        "label": "EmmyLua New Debug",
                        "description": "EmmyLua New Debug",
                        "body": {
                            "type": "emmylua_new",
                            "request": "launch",
                            "name": "EmmyLua New Debug",
                            "host": "localhost",
                            "port": 9966,
                            "ext": [
                                ".lua",
                                ".lua.txt",
                                ".lua.bytes"
                            ],
                            "ideConnectDebugger": true
                        }
                    }
                ],
                "languages": [
                    "lua"
                ]
            }
        ],
        "views": {
            "explorer": [
                {
                    "id": "emmylua.luarocks",
                    "name": "LuaRocks",
                    "when": "resourceExtname == .lua",
                    "icon": "$(package)"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "emmylua-luarocks",
                    "title": "LuaRocks",
                    "icon": "$(package)"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "emmylua.luarocks.refreshPackages",
                    "when": "view == emmylua.luarocks",
                    "group": "navigation@1"
                },
                {
                    "command": "emmylua.luarocks.searchPackages",
                    "when": "view == emmylua.luarocks",
                    "group": "navigation@2"
                },
                {
                    "command": "emmylua.luarocks.clearSearch",
                    "when": "view == emmylua.luarocks",
                    "group": "navigation@3"
                }
            ],
            "view/item/context": [
                {
                    "command": "emmylua.luarocks.showPackageInfo",
                    "when": "view == emmylua.luarocks && (viewItem == installedPackage || viewItem == availablePackage)",
                    "group": "1@1"
                },
                {
                    "command": "emmylua.luarocks.installPackage",
                    "when": "view == emmylua.luarocks && viewItem == availablePackage",
                    "group": "inline@1"
                },
                {
                    "command": "emmylua.luarocks.uninstallPackage",
                    "when": "view == emmylua.luarocks && viewItem == installedPackage",
                    "group": "inline@1"
                }
            ],
            "commandPalette": [
                {
                    "command": "emmylua.luarocks.installPackage",
                    "when": "resourceExtname == .lua"
                },
                {
                    "command": "emmylua.luarocks.searchPackages",
                    "when": "resourceExtname == .lua"
                },
                {
                    "command": "emmylua.luarocks.checkInstallation",
                    "when": "resourceExtname == .lua"
                }
            ]
        },
